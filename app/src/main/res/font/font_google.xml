<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/googlesans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/googlesans_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="o" />

    <font
        android:font="@font/googlesans_medium"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/googlesans_medium"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="o" />
    <font
        android:font="@font/googlesans_mediumitalic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/googlesans_mediumitalic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:targetApi="o" />
    <font
        android:font="@font/googlesans_bold"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/googlesans_bold"
        app:fontStyle="normal"
        app:fontWeight="400"
        tools:targetApi="o" />
    <font
        android:font="@font/googlesans_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/googlesans_italic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:targetApi="o" />

    <font
        android:font="@font/googlesans_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/googlesans_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="400"
        tools:targetApi="o" />






</font-family>